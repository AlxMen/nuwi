{
  "plugins": [],
  "source": {
    "include": ["src","components","app","prisma","actions"],
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": "(node_modules|docs)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./docs"
  }
}